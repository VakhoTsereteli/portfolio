{"ast":null,"code":"import \"./CarencyCalculator.css\";\nimport MainComponent from \"../../components/Main/Main.vue\";\nexport default {\n  name: \"CurrencyCalculator\",\n  components: {\n    MainComponent\n  },\n  data() {\n    return {\n      currencyDatas: null,\n      fromName: \"USD\",\n      toName: \"GEL\",\n      fromAmount: \"\",\n      toAmount: \"\",\n      showData: false,\n      countryDatas: null,\n      test: null\n    };\n  },\n  methods: {\n    currencyChange() {\n      fetch(`https://api.fastforex.io/fetch-one?from=${this.fromName}&to=${this.toName}&api_key=67c030db8d-4cf9a781a1-rv2gbf`).then(res => res.json()).then(data => {\n        this.currencyDatas = data;\n        this.toAmount = Object.values(data.result)[0];\n        this.toAmount = this.fromAmount * this.toAmount;\n      });\n    },\n    GetCountryData() {\n      fetch(\"https://restcountries.com/v2/all\").then(res => res.json()).then(data => {\n        //var countryDatas = {};\n        this.countryDatas = data;\n      });\n    },\n    toggleDropdown() {\n      this.isDropdownOpen = !this.isDropdownOpen;\n    }\n  },\n  mounted() {\n    this.GetCountryData();\n  },\n  computed: {\n    currencyCode() {\n      if (this.countryData && this.countryData.currencies && this.countryData.currencies.length > 0) {\n        return this.countryData.currencies[0].code;\n      }\n      return null;\n    }\n  }\n};","map":{"version":3,"names":["MainComponent","name","components","data","currencyDatas","fromName","toName","fromAmount","toAmount","showData","countryDatas","test","methods","currencyChange","fetch","then","res","json","Object","values","result","GetCountryData","toggleDropdown","isDropdownOpen","mounted","computed","currencyCode","countryData","currencies","length","code"],"sources":["/home/vakho_tsereteli/portfolio/src/Projects/CurrencyCalculator/CarencyCalculator.vue"],"sourcesContent":["<template>\n    <MainComponent/>\n    <br>\n    <div class=\"main\">\n        <h1 class=\"title\">Currency Calculator</h1>\n        <div class=\"calculator\">\n            <br>\n            <div>\n                <li>\n                    <div class=\"custom-select\">\n                        <div class=\"select-selected\" @click=\"toggleDropdown\">\n                        {{ selectedOption.text }}\n                        </div>\n                        <div class=\"select-items\" v-if=\"isDropdownOpen\">\n                        <div class=\"select\" v-for=\"country in countryDatas\" :key=\"country\">\n                            <h6></h6>\n                        </div>\n                        </div>\n                    </div>\n                </li>\n            </div>\n            <button @click=\"currencyChange\" id=\"convert\">Convert</button>\n        </div>\n    </div>\n    <div v-for=\"country in countryDatas\" :key=\"country\" style=\"display: flex;\">\n        <img :src=\"country.flags.png\"/>\n        <h1 v-if=\"country.currencies && country.currencies.length > 0\">{{ country.currencies[0].code }}</h1>\n    </div>\n</template>\n\n<script>\n    import \"./CarencyCalculator.css\";\n    import MainComponent from \"../../components/Main/Main.vue\"\n\n    export default{\n        name: \"CurrencyCalculator\",\n        components: {\n            MainComponent\n        },\n        data(){\n            return{\n                currencyDatas: null,\n                fromName: \"USD\",\n                toName: \"GEL\",\n                fromAmount: \"\",\n                toAmount: \"\",\n                showData: false,\n                countryDatas: null,\n                test: null\n            };\n        },\n        methods: {\n            currencyChange(){\n                fetch(\n                    `https://api.fastforex.io/fetch-one?from=${this.fromName}&to=${this.toName}&api_key=67c030db8d-4cf9a781a1-rv2gbf`\n                    )\n                .then((res)=> res.json())\n                .then((data)=> {\n                    this.currencyDatas = data;\n\n                    this.toAmount = Object.values(data.result)[0]\n\n                    this.toAmount = this.fromAmount * this.toAmount\n                });\n            },\n            GetCountryData() {\n                fetch(\"https://restcountries.com/v2/all\")\n                    .then((res) => res.json())\n                    .then((data) => {\n                    //var countryDatas = {};\n                    this.countryDatas = data;\n                    });\n            },\n            toggleDropdown() {\n            this.isDropdownOpen = !this.isDropdownOpen;\n            }\n        },\n        mounted(){\n            this.GetCountryData()\n        },\n        computed: {\n            currencyCode() {\n            if (this.countryData && this.countryData.currencies && this.countryData.currencies.length > 0) {\n                return this.countryData.currencies[0].code;\n            }\n            return null;\n            }\n        }\n    };\n</script>"],"mappings":"AA+BI,OAAO,yBAAyB;AAChC,OAAOA,aAAY,MAAO,gCAA+B;AAEzD,eAAc;EACVC,IAAI,EAAE,oBAAoB;EAC1BC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAIA,CAAA,EAAE;IACF,OAAM;MACFC,aAAa,EAAE,IAAI;MACnBC,QAAQ,EAAE,KAAK;MACfC,MAAM,EAAE,KAAK;MACbC,UAAU,EAAE,EAAE;MACdC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,KAAK;MACfC,YAAY,EAAE,IAAI;MAClBC,IAAI,EAAE;IACV,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACLC,cAAcA,CAAA,EAAE;MACZC,KAAK,CACA,2CAA0C,IAAI,CAACT,QAAS,OAAM,IAAI,CAACC,MAAO,uCAC3E,EACHS,IAAI,CAAEC,GAAG,IAAIA,GAAG,CAACC,IAAI,CAAC,CAAC,EACvBF,IAAI,CAAEZ,IAAI,IAAI;QACX,IAAI,CAACC,aAAY,GAAID,IAAI;QAEzB,IAAI,CAACK,QAAO,GAAIU,MAAM,CAACC,MAAM,CAAChB,IAAI,CAACiB,MAAM,CAAC,CAAC,CAAC;QAE5C,IAAI,CAACZ,QAAO,GAAI,IAAI,CAACD,UAAS,GAAI,IAAI,CAACC,QAAO;MAClD,CAAC,CAAC;IACN,CAAC;IACDa,cAAcA,CAAA,EAAG;MACbP,KAAK,CAAC,kCAAkC,EACnCC,IAAI,CAAEC,GAAG,IAAKA,GAAG,CAACC,IAAI,CAAC,CAAC,EACxBF,IAAI,CAAEZ,IAAI,IAAK;QAChB;QACA,IAAI,CAACO,YAAW,GAAIP,IAAI;MACxB,CAAC,CAAC;IACV,CAAC;IACDmB,cAAcA,CAAA,EAAG;MACjB,IAAI,CAACC,cAAa,GAAI,CAAC,IAAI,CAACA,cAAc;IAC1C;EACJ,CAAC;EACD