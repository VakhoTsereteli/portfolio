{"ast":null,"code":"import \"./CarencyCalculator.css\";\nimport MainComponent from \"../../components/Main/Main.vue\";\nexport default {\n  name: \"CurrencyCalculator\",\n  components: {\n    MainComponent\n  },\n  data() {\n    return {\n      currency: {\n        gel: {\n          \"name\": \"GEL\"\n        },\n        usd: {\n          \"name\": \"USD\",\n          \"price\": 20\n        },\n        eur: {\n          \"name\": \"EUR\",\n          \"price\": 10\n        }\n      },\n      currencyDatas: \"\",\n      fromName: \"USD\",\n      toName: \"GEL\",\n      fromAmount: \"\",\n      toAmount: \"\",\n      showData: false\n    };\n  },\n  methods: {\n    currencyData() {\n      fetch(`https://api.fastforex.io/fetch-one?from=${this.fromName}&to=${this.toName}&api_key=67c030db8d-4cf9a781a1-rv2gbf`).then(res => res.json()).then(data => {\n        this.currencyDatas = data;\n        this.toAmount = Object.values(data.result)[0];\n        this.toAmount = this.fromAmount * this.toAmount;\n        this.showData = true;\n      });\n    },\n    countriesDatas() {\n      fetch(\"https://restcountries.com/v3.1/all\").then(res => res.json);\n    }\n  },\n  mounted() {}\n};","map":{"version":3,"names":["MainComponent","name","components","data","currency","gel","usd","eur","currencyDatas","fromName","toName","fromAmount","toAmount","showData","methods","currencyData","fetch","then","res","json","Object","values","result","countriesDatas","mounted"],"sources":["/home/vakho_tsereteli/portfolio/src/Projects/CurrencyCalculator/CarencyCalculator.vue"],"sourcesContent":["<template>\n    <MainComponent/>\n    <br>\n    <div class=\"main\">\n        <h1 class=\"title\">Currency Calculator</h1>\n        <div class=\"calculator\">\n            <br>\n            <div>\n                <li>\n                    <select v-model=\"fromName\">\n                        <option v-for=\"(currency, index) in currency\" :key=\"index\">{{ currency.name }}</option>\n                    </select>\n                    <br>\n                    <input type=\"text\" v-model=\"fromAmount\"/>\n                </li>\n                <li>\n                    <select v-model=\"toName\">\n                        <option v-for=\"(currency, index) in currency\" :key=\"index\">{{ currency.name }}</option>\n                    </select>\n                    <br>\n                    <input type=\"text\" v-model=\"toAmount\"/>\n                </li>\n            </div>\n            <button @click=\"currencyData\" id=\"convert\">Convert</button>\n        </div>\n    </div>\n</template>\n\n<script>\n    import \"./CarencyCalculator.css\";\n    import MainComponent from \"../../components/Main/Main.vue\"\n\n    export default{\n        name: \"CurrencyCalculator\",\n        components: {\n            MainComponent\n        },\n        data(){\n            return{\n                currency:{\n                    gel:{\"name\": \"GEL\", },\n                    usd:{\"name\": \"USD\", \"price\": 20},\n                    eur:{\"name\": \"EUR\", \"price\": 10}\n                },\n                currencyDatas: \"\",\n                fromName: \"USD\",\n                toName: \"GEL\",\n                fromAmount: \"\",\n                toAmount: \"\",\n                showData: false\n            };\n        },\n        methods: {\n            currencyData(){\n                fetch(\n                    `https://api.fastforex.io/fetch-one?from=${this.fromName}&to=${this.toName}&api_key=67c030db8d-4cf9a781a1-rv2gbf`\n                    )\n                .then((res)=> res.json())\n                .then((data)=> {\n                    this.currencyDatas = data;\n\n                    this.toAmount = Object.values(data.result)[0]\n\n                    this.toAmount = this.fromAmount * this.toAmount\n                    this.showData = true\n                });\n            },\n            countriesDatas(){\n                fetch(\n                    \"https://restcountries.com/v3.1/all\"\n                )\n                .then((res)=> res.json)\n            }\n        },\n        mounted(){\n        }\n    };\n</script>"],"mappings":"AA6BI,OAAO,yBAAyB;AAChC,OAAOA,aAAY,MAAO,gCAA+B;AAEzD,eAAc;EACVC,IAAI,EAAE,oBAAoB;EAC1BC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAIA,CAAA,EAAE;IACF,OAAM;MACFC,QAAQ,EAAC;QACLC,GAAG,EAAC;UAAC,MAAM,EAAE;QAAO,CAAC;QACrBC,GAAG,EAAC;UAAC,MAAM,EAAE,KAAK;UAAE,OAAO,EAAE;QAAE,CAAC;QAChCC,GAAG,EAAC;UAAC,MAAM,EAAE,KAAK;UAAE,OAAO,EAAE;QAAE;MACnC,CAAC;MACDC,aAAa,EAAE,EAAE;MACjBC,QAAQ,EAAE,KAAK;MACfC,MAAM,EAAE,KAAK;MACbC,UAAU,EAAE,EAAE;MACdC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE;IACd,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACLC,YAAYA,CAAA,EAAE;MACVC,KAAK,CACA,2CAA0C,IAAI,CAACP,QAAS,OAAM,IAAI,CAACC,MAAO,uCAC3E,EACHO,IAAI,CAAEC,GAAG,IAAIA,GAAG,CAACC,IAAI,CAAC,CAAC,EACvBF,IAAI,CAAEd,IAAI,IAAI;QACX,IAAI,CAACK,aAAY,GAAIL,IAAI;QAEzB,IAAI,CAACS,QAAO,GAAIQ,MAAM,CAACC,MAAM,CAAClB,IAAI,CAACmB,MAAM,CAAC,CAAC,CAAC;QAE5C,IAAI,CAACV,QAAO,GAAI,IAAI,CAACD,UAAS,GAAI,IAAI,CAACC,QAAO;QAC9C,IAAI,CAACC,QAAO,GAAI,IAAG;MACvB,CAAC,CAAC;IACN,CAAC;IACDU,cAAcA,CAAA,EAAE;MACZP,KAAK,CACD,oCACJ,EACCC,IAAI,CAAEC,GAAG,IAAIA,GAAG,CAACC,IAAI;IAC1B;EACJ,CAAC;EACDK,OAAOA,CAAA,EAAE,CACT;AACJ,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}