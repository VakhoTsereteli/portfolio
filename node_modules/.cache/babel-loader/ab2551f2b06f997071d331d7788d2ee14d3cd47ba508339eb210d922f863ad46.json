onst country = this.countryDatas[key]\n                    array.push(country.currencies)\n                }\n               // console.log(array)\n                this.test = array;\n                \n\n                var x = []\n                const objects = this.test.filter(object => object !== undefined && object !== null);\n                for (const object of objects) {\n                    // Get the object keys\n                    const keys = Object.keys(object).map(key => key);\n                    // Print the object keys\n                    x.push(keys)\n                }\n                x.sort()\n                this.currencies = x;\n                this.showData = true\n                \n                })\n            },\n        },\n        mounted(){\n            this.countryData()\n            var CountryFlag = Object.assign(this.countryDatas,this.currencies)\n            console.log(CountryFlag)\n        },\n        computed: {\n            CarrencyFlag(){\n                return { ...this.countryDatas, ...this.currencies}\n            }\n            \n        }\n    };\n</script>"],"mappings":";AA6BI,OAAO,yBAAyB;AAChC,OAAOA,aAAY,MAAO,gCAA+B;AAEzD,eAAc;EACVC,IAAI,EAAE,oBAAoB;EAC1BC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAIA,CAAA,EAAE;IACF,OAAM;MACFC,QAAQ,EAAC;QACLC,GAAG,EAAC;UAAC,MAAM,EAAE;QAAO,CAAC;QACrBC,GAAG,EAAC;UAAC,MAAM,EAAE,KAAK;UAAE,OAAO,EAAE;QAAE,CAAC;QAChCC,GAAG,EAAC;UAAC,MAAM,EAAE,KAAK;UAAE,OAAO,EAAE;QAAE;MACnC,CAAC;MACDC,aAAa,EAAE,IAAI;MACnBC,QAAQ,EAAE,KAAK;MACfC,MAAM,EAAE,KAAK;MACbC,UAAU,EAAE,EAAE;MACdC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,KAAK;MACfC,YAAY,EAAE,IAAI;MAClBC,IAAI,EAAE,EAAE;MACRC,UAAU,EAAE,IAAI;MAChBC,WAAW,EAAE;IACjB,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACLC,YAAYA,CAAA,EAAE;MACVC,KAAK,CACA,2CAA0C,IAAI,CAACX,QAAS,OAAM,IAAI,CAACC,MAAO,uCAC3E,EACHW,IAAI,CAAEC,GAAG,IAAIA,GAAG,CAACC,IAAI,CAAC,CAAC,EACvBF,IAAI,CAAElB,IAAI,IAAI;QACX,IAAI,CAACK,aAAY,GAAIL,IAAI;QAEzB,IAAI,CAACS,QAAO,GAAIY,MAAM,CAACC,MAAM,CAACtB,IAAI,CAACuB,MAAM,CAAC,CAAC,CA