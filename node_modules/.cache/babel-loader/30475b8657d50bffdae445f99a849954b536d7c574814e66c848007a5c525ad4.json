{"ast":null,"code":"import currencies from \"./currencies.json\";\nimport \"./CarencyCalculator.css\";\nimport MainComponent from \"../../components/Main/Main.vue\";\nexport default {\n  name: \"CurrencyCalculator\",\n  components: {\n    MainComponent\n  },\n  data() {\n    return {\n      fromAmount: null,\n      toAmount: null,\n      countryDatas: null,\n      fromSelected: null,\n      showFromMenu: false,\n      toSelected: null,\n      showToMenu: false,\n      currencies: currencies\n    };\n  },\n  methods: {\n    currencyChange() {\n      fetch(`https://v6.exchangerate-api.com/v6/4f97de937a2f5ca4c59a5c1c/latest/${this.fromSelected && this.fromSelected.currencies[0].code}`).then(res => res.json()).then(data => {\n        this.toAmount = data.conversion_rates[this.toSelected.currencies[0].code];\n        this.toAmount = this.fromAmount * this.toAmount;\n        //var x = []\n        //x.push(data.conversion_rates)\n      });\n    },\n\n    GetCountryData() {\n      fetch(\"https://restcountries.com/v2/all\").then(res => res.json()).then(data => {\n        this.countryDatas = data;\n      });\n    },\n    toggleDropdown() {\n      this.isDropdownOpen = !this.isDropdownOpen;\n    },\n    SelectFrom(country) {\n      this.fromSelected = country;\n      this.showFromMenu = false;\n    },\n    SelectTo(country) {\n      this.toSelected = country;\n      this.showToMenu = false;\n    }\n  },\n  mounted() {\n    this.GetCountryData();\n  },\n  computed: {}\n};","map":{"version":3,"names":["currencies","MainComponent","name","components","data","fromAmount","toAmount","countryDatas","fromSelected","showFromMenu","toSelected","showToMenu","methods","currencyChange","fetch","code","then","res","json","conversion_rates","GetCountryData","toggleDropdown","isDropdownOpen","SelectFrom","country","SelectTo","mounted","computed"],"sources":["/home/vakho_tsereteli/portfolio/src/Projects/CurrencyCalculator/CarencyCalculator.vue"],"sourcesContent":["<template>\n    <MainComponent/>\n    <br>\n    <div class=\"main\">\n        <h1 class=\"title\">Currency Calculator</h1>\n        <div class=\"currencies\">\n            \n        </div>\n        <div class=\"calculator\">\n            <div>\n                <div>\n                    <button class=\"choose\" @click=\"showFromMenu = !showFromMenu\">\n                        <img :src=\"fromSelected && fromSelected.flags.png\" v-if=\"fromSelected\" />\n                        {{ fromSelected && fromSelected.currencies[0].code }}\n                        <i class=\"bi bi-caret-up-fill arrow\" v-if=\"showFromMenu\"></i>\n                        <i class=\"bi bi-caret-down-fill arrow\" v-else></i>\n                    </button>\n                    <br>\n                    <div v-show=\"showFromMenu\">\n                        <li v-for=\"currency in currencies\" :key=\"currency\" @click=\"SelectFrom(country)\">\n                            <h2 style=\"margin-left: 30px;\">{{ currency.flag }}</h2>\n                            <h2 style=\"position: absolute; right: 0;\">\n                                {{ currency.code }}\n                            </h2>\n                        </li>\n                    </div>\n                    <br>\n                    <label>\n                        <h4>Amount From</h4>\n                    </label>\n                    <input type=\"text\" v-model=\"fromAmount\"/>\n                </div>\n                <div>\n                    <button class=\"choose\" @click=\"showToMenu = !showToMenu\">\n                        <img :src=\"toSelected && toSelected.flags.png\" v-if=\"toSelected\" />\n                        {{ toSelected && toSelected.currencies[0].code }}\n                        <i class=\"bi bi-caret-up-fill arrow\" v-if=\"showToMenu\"></i>\n                        <i class=\"bi bi-caret-down-fill arrow\" v-else></i>\n                    </button>\n                    <br>\n                    <div v-show=\"showToMenu\">\n                        <li v-for=\"currency in currencies\" :key=\"currency\" >\n                            <h2 style=\"margin-left: 30px;\">{{ currency.flag }}</h2>\n                            <h2 style=\"position: absolute; right: 0;\">\n                                {{ currency.code }}\n                            </h2>\n                        </li>\n                    </div>\n                    <br>\n                    <label>\n                        <h4>Amount From</h4>\n                    </label>\n                    <input type=\"text\" v-model=\"toAmount\"/>\n                </div>\n            </div>\n            <button @click=\"currencyChange\" id=\"convert\">Convert</button>\n        </div>\n        <h1 v-for=\"currency in currencies\" :key=\"currency\">{{ currency.flag+\" \"+currency.code }}</h1>\n    </div>\n</template>\n\n<script>\n    import currencies from \"./currencies.json\"\n    import \"./CarencyCalculator.css\";\n    import MainComponent from \"../../components/Main/Main.vue\"\n\n    export default{\n        name: \"CurrencyCalculator\",\n        components: {\n            MainComponent\n        },\n        data(){\n            return{\n                fromAmount: null,\n                toAmount: null,\n                countryDatas: null,\n                fromSelected: null,\n                showFromMenu: false,\n                toSelected: null,\n                showToMenu: false,\n                currencies: currencies\n            };\n        },\n        methods: {\n            currencyChange(){\n                fetch(\n                    `https://v6.exchangerate-api.com/v6/4f97de937a2f5ca4c59a5c1c/latest/${this.fromSelected && this.fromSelected.currencies[0].code}`\n                    )\n                .then((res)=> res.json())\n                .then((data)=> {\n\n                    this.toAmount = data.conversion_rates[this.toSelected.currencies[0].code]\n\n                    this.toAmount = this.fromAmount * this.toAmount\n                    //var x = []\n                    //x.push(data.conversion_rates)\n                });\n            },\n            GetCountryData() {\n                fetch(\"https://restcountries.com/v2/all\")\n                    .then((res) => res.json())\n                    .then((data) => {\n                    this.countryDatas = data;\n                    });\n            },\n            toggleDropdown() {\n            this.isDropdownOpen = !this.isDropdownOpen;\n            },\n            SelectFrom(country){\n                this.fromSelected = country\n                this.showFromMenu = false\n            },\n            SelectTo(country){\n                this.toSelected = country\n                this.showToMenu = false\n            }\n        },\n        mounted(){\n            this.GetCountryData()\n        },\n        computed: {\n            \n        }\n    };\n</script>"],"mappings":"AA8DI,OAAOA,UAAS,MAAO,mBAAkB;AACzC,OAAO,yBAAyB;AAChC,OAAOC,aAAY,MAAO,gCAA+B;AAEzD,eAAc;EACVC,IAAI,EAAE,oBAAoB;EAC1BC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAIA,CAAA,EAAE;IACF,OAAM;MACFC,UAAU,EAAE,IAAI;MAChBC,QAAQ,EAAE,IAAI;MACdC,YAAY,EAAE,IAAI;MAClBC,YAAY,EAAE,IAAI;MAClBC,YAAY,EAAE,KAAK;MACnBC,UAAU,EAAE,IAAI;MAChBC,UAAU,EAAE,KAAK;MACjBX,UAAU,EAAEA;IAChB,CAAC;EACL,CAAC;EACDY,OAAO,EAAE;IACLC,cAAcA,CAAA,EAAE;MACZC,KAAK,CACA,sEAAqE,IAAI,CAACN,YAAW,IAAK,IAAI,CAACA,YAAY,CAACR,UAAU,CAAC,CAAC,CAAC,CAACe,IAAK,EAChI,EACHC,IAAI,CAAEC,GAAG,IAAIA,GAAG,CAACC,IAAI,CAAC,CAAC,EACvBF,IAAI,CAAEZ,IAAI,IAAI;QAEX,IAAI,CAACE,QAAO,GAAIF,IAAI,CAACe,gBAAgB,CAAC,IAAI,CAACT,UAAU,CAACV,UAAU,CAAC,CAAC,CAAC,CAACe,IAAI;QAExE,IAAI,CAACT,QAAO,GAAI,IAAI,CAACD,UAAS,GAAI,IAAI,CAACC,QAAO;QAC9C;QACA;MACJ,CAAC,CAAC;IACN,CAAC;;IACDc,cAAcA,CAAA,EAAG;MACbN,KAAK,CAAC,kCAAkC,EACnCE,IAAI,CAAEC,GAAG,IAAKA,GAAG,CAACC,IAAI,CAAC,CAAC,EACxBF,IAAI,CAAEZ,IAAI,IAAK;QAChB,IAAI,CAACG,YAAW,GAAIH,IAAI;MACxB,CAAC,CAAC;IACV,CAAC;IACDiB,cAAcA,CAAA,EAAG;MACjB,IAAI,CAACC,cAAa,GAAI,CAAC,IAAI,CAACA,cAAc;IAC1C,CAAC;IACDC,UAAUA,CAACC,OAAO,EAAC;MACf