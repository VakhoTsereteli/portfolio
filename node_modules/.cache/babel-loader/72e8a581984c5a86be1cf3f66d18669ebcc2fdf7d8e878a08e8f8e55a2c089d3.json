{"ast":null,"code":"import \"./CarencyCalculator.css\";\nimport MainComponent from \"../../components/Main/Main.vue\";\nimport currency from \"./currency.json\";\nexport default {\n  name: \"CurrencyCalculator\",\n  components: {\n    MainComponent\n  },\n  data() {\n    return {\n      currency: currency,\n      currencyDatas: null,\n      fromName: \"USD\",\n      toName: \"GEL\",\n      fromAmount: \"\",\n      toAmount: \"\",\n      showData: false\n    };\n  },\n  methods: {\n    currencyData() {\n      fetch(`https://api.fastforex.io/fetch-one?from=${this.fromName}&to=${this.toName}&api_key=67c030db8d-4cf9a781a1-rv2gbf`).then(res => res.json()).then(data => {\n        localStorage.setItem(\"currencyDatas\", JSON.stringify(data));\n        var ParsedCurrencyDatas = JSON.parse(localStorage.getItem(\"currencyDatas\"));\n        this.currencyDatas = ParsedCurrencyDatas;\n        this.showData = true;\n        this.toAmount = this.fromAmount * this.currencyDatas.result[Object.keys(this.currencyDatas.result)[0]];\n      });\n      fetch(\"https://api.fastforex.io/fetch-all?from=USD&api_key=67c030db8d-4cf9a781a1-rv2gbf\").then(res => res.json()).then(data => {\n        console.log(data);\n      });\n    }\n  },\n  mounted() {}\n};","map":{"version":3,"names":["MainComponent","currency","name","components","data","currencyDatas","fromName","toName","fromAmount","toAmount","showData","methods","currencyData","fetch","then","res","json","localStorage","setItem","JSON","stringify","ParsedCurrencyDatas","parse","getItem","result","Object","keys","console","log","mounted"],"sources":["/home/vakho_tsereteli/portfolio/src/Projects/CurrencyCalculator/CarencyCalculator.vue"],"sourcesContent":["<template>\n    <MainComponent/>\n    <br>\n    <div class=\"main\">\n        <h1 class=\"title\">Currency Calculator</h1>\n        <div class=\"calculator\">\n            <br>\n            <div>\n                <li>\n                    <select v-model=\"fromName\">\n                        <option v-for=\"(currency, index) in currency\" :key=\"index\">{{ currency.name }}</option>\n                    </select>\n                    <br>\n                    <input type=\"text\" v-model=\"fromAmount\"/>\n                </li>\n                <li>\n                    <select v-model=\"toName\">\n                        <option v-for=\"(currency, index) in currency\" :key=\"index\">{{ currency.name }}</option>\n                    </select>\n                    <br>\n                    <input type=\"text\" v-model=\"toAmount\"/>\n                </li>\n            </div>\n            <button @click=\"currencyData\" id=\"convert\">Convert</button>\n            <h1 v-if=\"showData\">{{ currencyDatas.base }}</h1>\n            <h1 v-if=\"showData\">{{ toAmount }}</h1>\n        </div>\n    </div>\n</template>\n\n<script>\n    import \"./CarencyCalculator.css\";\n    import MainComponent from \"../../components/Main/Main.vue\";\n    import currency from \"./currency.json\"\n\n    export default{\n        name: \"CurrencyCalculator\",\n        components: {\n            MainComponent\n        },\n        data(){\n            return{\n                currency: currency,\n                currencyDatas: null,\n                fromName: \"USD\",\n                toName: \"GEL\",\n                fromAmount: \"\",\n                toAmount: \"\",\n                showData: false\n            };\n        },\n        methods: {\n            currencyData(){\n                fetch(\n                    `https://api.fastforex.io/fetch-one?from=${this.fromName}&to=${this.toName}&api_key=67c030db8d-4cf9a781a1-rv2gbf`\n                    )\n                .then((res)=> res.json())\n                .then((data)=> {\n                    localStorage.setItem(\"currencyDatas\", JSON.stringify(data));\n                    var ParsedCurrencyDatas = JSON.parse(localStorage.getItem(\"currencyDatas\"));\n                    this.currencyDatas = ParsedCurrencyDatas;\n      