      });\n            },\n            /* GetCountryData(){\n                fetch(\n                    \"https://restcountries.com/v3.1/all\"\n                )\n                .then((res)=> res.json())\n                .then((data)=>{\n\n                var countryDatas = {};\n                countryDatas = {...countryDatas, data}\n                \n                var currencyKeys =[]\n                for (let i = 0; i < this.countryDatas.data.length; i++) {\n                    if (this.countryDatas.data[i].currencies) {\n                        const currencyKey = Object.keys(data[i].currencies)[0];\n                        currencyKeys.push(currencyKey)\n                    }\n                } \n                countryDatas = {...countryDatas, currencyKeys}\n                this.countryDatas = countryDatas;\n                console.log(this.countryDatas)\n                \n                })\n            } */\n            GetCountryData() {\n                fetch(\"https://restcountries.com/v2/all\")\n                    .then((res) => res.json())\n                    .then((data) => {\n                    //var countryDatas = {};\n                    this.countryDatas = data;\n                    });\n            },\n            toggleDropdown() {\n            this.isDropdownOpen = !this.isDropdownOpen;\n            }\n        },\n        mounted(){\n            this.GetCountryData()\n        },\n    };\n</script>"],"mappings":"AAuBI,OAAO,yBAAyB;AAChC,OAAOA,aAAY,MAAO,gCAA+B;AAEzD,eAAc;EACVC,IAAI,EAAE,oBAAoB;EAC1BC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAIA,CAAA,EAAE;IACF,OAAM;MACFC,aAAa,EAAE,IAAI;MACnBC,QAAQ,EAAE,KAAK;MACfC,MAAM,EAAE,KAAK;MACbC,UAAU,EAAE,EAAE;MACdC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,KAAK;MACfC,YAAY,EAAE,IAAI;MAClBC,IAAI,EAAE;IACV,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACLC,cAAcA,CAAA,EAAE;MACZC,KAAK,CACA,2CAA0C,IAAI,CAACT,QAAS,OAAM,IAAI,CAACC,MAAO,uCAC3E,EACHS,IAAI,CAAEC,GAAG,IAAIA,GAAG,CAACC,IAAI,CAAC,CAAC,EACvBF,IAAI,CAAEZ,IAAI,IAAI;QACX,IAAI,CAACC,aAAY