{"ast":null,"code":"import \"./CarencyCalculator.css\";\nimport MainComponent from \"../../components/Main/Main.vue\";\nexport default {\n  name: \"CurrencyCalculator\",\n  components: {\n    MainComponent\n  },\n  data() {\n    return {\n      fromName: \"USD\",\n      toName: \"GEL\",\n      fromAmount: \"\",\n      toAmount: \"\",\n      countryDatas: null,\n      fromSelected: null,\n      showFromMenu: false,\n      toSelected: null,\n      showToMenu: false\n    };\n  },\n  methods: {\n    currencyChange() {\n      fetch(`https://api.fastforex.io/fetch-one?from=${this.fromName}&to=${this.toName}&api_key=67c030db8d-4cf9a781a1-rv2gbf`).then(res => res.json()).then(data => {\n        this.currencyDatas = data;\n        this.toAmount = Object.values(data.result)[0];\n        this.toAmount = this.fromAmount * this.toAmount;\n      });\n    },\n    GetCountryData() {\n      fetch(\"https://restcountries.com/v2/all\").then(res => res.json()).then(data => {\n        this.countryDatas = data;\n      });\n    },\n    toggleDropdown() {\n      this.isDropdownOpen = !this.isDropdownOpen;\n    },\n    SelectFrom(country) {\n      this.fromSelected = country;\n      this.showToMenu = false;\n    },\n    SelectTo(country) {\n      this.toSelected = country;\n      this.showFromMenu = false;\n    }\n  },\n  mounted() {\n    this.GetCountryData();\n  },\n  computed: {\n    currencyCode() {\n      if (this.countryData && this.countryData.currencies && this.countryData.currencies.length > 0) {\n        return this.countryData.currencies[0].code;\n      }\n      return null;\n    }\n  }\n};","map":{"version":3,"names":["MainComponent","name","components","data","fromName","toName","fromAmount","toAmount","countryDatas","fromSelected","showFromMenu","toSelected","showToMenu","methods","currencyChange","fetch","then","res","json","currencyDatas","Object","values","result","GetCountryData","toggleDropdown","isDropdownOpen","SelectFrom","country","SelectTo","mounted","computed","currencyCode","countryData","currencies","length","code"],"sources":["/home/vakho_tsereteli/portfolio/src/Projects/CurrencyCalculator/CarencyCalculator.vue"],"sourcesContent":["<template>\n    <MainComponent/>\n    <br>\n    <div class=\"main\">\n        <h1 class=\"title\">Currency Calculator</h1>\n        <div class=\"calculator\">\n            <div>\n                <div>\n                    <button class=\"choose\" @click=\"showFromMenu = !showFromMenu\">\n                        <img :src=\"fromSelected && fromSelected.flags.png\" v-if=\"fromSelected\" />\n                        {{ fromSelected && fromSelected.currencies[0].code }}\n                        <i class=\"bi bi-caret-up-fill arrow\" v-if=\"showFromMenu\"></i>\n                        <i class=\"bi bi-caret-down-fill arrow\" v-else></i>\n                    </button>\n                    <br>\n                    <div v-show=\"showFromMenu\">\n                        <li v-for=\"country in countryDatas\" :key=\"country\" @click=\"SelectFrom(country)\">\n                            <img :src=\"country.flags.png\"/>\n                            <h2 v-if=\"country.currencies && country.currencies.length > 0\">\n                                {{country.currencies[0].code }}\n                            </h2>\n                        </li>\n                    </div>\n                    <br>\n                    <label>\n                        <h4>Amount From</h4>\n                    </label>\n                    <input type=\"text\" :v-model=\"fromAmount\"/>\n                </div>\n                <div>\n                    <button class=\"choose\" @click=\"showFromMenu = !showFromMenu\">\n                        <img :src=\"fromSelected && fromSelected.flags.png\" v-if=\"fromSelected\" />\n                        {{ toSelected && toSelected.currencies[0].code }}\n                        <i class=\"bi bi-caret-up-fill arrow\" v-if=\"showFromMenu\"></i>\n                        <i class=\"bi bi-caret-down-fill arrow\" v-else></i>\n                    </button>\n                    <br>\n                    <div v-show=\"showFromMenu\">\n                        <li v-for=\"country in countryDatas\" :key=\"country\" @click=\"SelectTo(country)\">\n                            <img :src=\"country.flags.png\"/>\n                            <h2 v-if=\"country.currencies && country.currencies.length > 0\">\n                                {{country.currencies[0].code }}\n                            </h2>\n                        </li>\n                    </div>\n                    <br>\n                    <label>\n                        <h4>Amount From</h4>\n                    </label>\n                    <input type=\"text\" :v-model=\"fromAmount\"/>\n                </div>\n            </div>\n            <button @click=\"currencyChange\" id=\"convert\">Convert</button>\n        </div>\n    </div>\n    <div id=\"test\">\n        <div v-for=\"country in countryDatas\" :key=\"country\" style=\"display: flex;\">\n            <img :src=\"country.flags.png\"/>\n            <h1 v-if=\"country.currencies && country.currencies.length > 0\">{{ country.currencies[0].code }}</h1>\n        </div>\n    </div>\n</template>\n\n<script>\n    import \"./CarencyCalculator.css\";\n    import MainComponent from \"../../components/Main/Main.vue\"\n\n    export default{\n        name: \"CurrencyCalculator\",\n        components: {\n            MainComponent\n        },\n        data(){\n            return{\n                fromName: \"USD\",\n                toName: \"GEL\",\n                fromAmount: \"\",\n                toAmount: \"\",\n                countryDatas: null,\n                fromSelected: null,\n                showFromMenu: false,\n                toSelected: null,\n                showToMenu: false\n            };\n        },\n        methods: {\n            currencyChange(){\n                fetch(\n                    `https://api.fastforex.io/fetch-one?from=${this.fromName}&to=${this.toName}&api_key=67c030db8d-4cf9a781a1-rv2gbf`\n                    )\n                .then((res)=> res.json())\n                .then((data)=> {\n                    this.currencyDatas = data;\n\n                    this.toAmount = Object.values(data.result)[0]\n\n                    this.toAmount = this.fromAmount * this.toAmount\n                });\n            },\n            GetCountryData() {\n                fetch(\"https://restcountries.com/v2/all\")\n                    .then((res) => res.json())\n                    .then((data) => {\n                    this.countryDatas = data;\n                    });\n            },\n            toggleDropdown() {\n            this.isDropdownOpen = !this.isDropdownOpen;\n            },\n            SelectFrom(country){\n                this.fromSelected = country\n                this.showToMenu = false\n            },\n            SelectTo(country){\n                this.toSelected = country\n                this.showFromMenu = false\n            }\n        },\n        mounted(){\n            this.GetCountryData()\n        },\n        computed: {\n            currencyCode() {\n            if (this.countryData && this.countryData.currencies && this.countryData.currencies.length > 0) {\n                return this.countryData.currencies[0].code;\n            }\n            return null;\n            }\n        }\n    };\n</script>"],"mappings":"AAgEI,OAAO,yBAAyB;AAChC,OAAOA,aAAY,MAAO,gCAA+B;AAEzD,eAAc;EACVC,IAAI,EAAE,oBAAoB;EAC1BC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAIA,CAAA,EAAE;IACF,OAAM;MACFC,QAAQ,EAAE,KAAK;MACfC,MAAM,EAAE,KAAK;MACbC,UAAU,EAAE,EAAE;MACdC,QAAQ,EAAE,EAAE;MACZC,YAAY,EAAE,IAAI;MAClBC,YAAY,EAAE,IAAI;MAClBC,YAAY,EAAE,KAAK;MACnBC,UAAU,EAAE,IAAI;MAChBC,UAAU,EAAE;IAChB,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACLC,cAAcA,CAAA,EAAE;MACZC,KAAK,CACA,2CAA0C,IAAI,CAACX,QAAS,OAAM,IAAI,CAACC,MAAO,uCAC3E,EACHW,IAAI,CAAEC,GAAG,IAAIA,GAAG,CAACC,IAAI,CAAC,CAAC,EACvBF,IAAI,CAAEb,IAAI,IAAI;QACX,IAAI,CAACgB,aA