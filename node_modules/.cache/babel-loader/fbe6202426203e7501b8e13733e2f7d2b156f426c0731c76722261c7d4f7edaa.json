              this.toAmount = this.fromAmount * this.currencyDatas.result[Object.keys(this.currencyDatas.result)[0]]\n                    \n                });\n                fetch(\n                    \"https://countryapi.io/api/all?apikey=TH0Zsojmqn1ywrsJvToGgUlhZWu1IUbwxOPCQmxu\"\n                )\n                .then((res)=> res.json())\n                .then((data)=>{\n\n                this.countryData = [data];\n                console.log(this.countryData[0])\n                })\n                this.showData = true\n            },\n        },\n        mounted(){\n        }\n    };\n</script>"],"mappings":"AA8BI,OAAO,yBAAyB;AAChC,OAAOA,aAAY,MAAO,gCAAgC;AAC1D,OAAOC,QAAO,MAAO,iBAAgB;AAErC,eAAc;EACVC,IAAI,EAAE,oBAAoB;EAC1BC,UAAU,EAAE;IACRH;EACJ,CAAC;EACDI,IAAIA,CAAA,EAAE;IACF,OAAM;MACFH,QAAQ,EAAEA,QAAQ;MAClBI,aAAa,EAAE,IAAI;MACnBC,QAAQ,EAAE,KAAK;MACfC,MAAM,EAAE,KAAK;MACbC,UAAU,EAAE,EAAE;MACdC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,KAAK;MACfC,WAAW,EAAE;IACjB,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACLC,YAAYA,CAAA,EAAE;MACVC,KAAK,CACA,2CAA0C,IAAI,CAACR,QAAS,OAAM,IAAI,CAACC,MAAO,uCAC3E,EACHQ,IAAI,CAAEC,GAAG,IAAIA,GAAG,CAACC,IAAI,CAAC,CAAC,EACvBF,IAAI,CAAEX,IAAI,IAAI;QACXc,YAAY,CAACC,OAAO,CAAC,eAAe,EAAEC,IAAI,CAACC,SAAS,CAACjB,IAAI,CAAC,CAAC;QAC3D,IAAIkB,mBAAkB,GAAIF,IAAI,CAACG,KAAK,CAACL,YAAY,CAACM,OAAO,CAAC,eAAe,CAAC,CAAC;QAC3E,IAAI,CAACnB,aAAY,GAAIiB,mBAAmB;QACxC,IAAI,CAACb,QAAO,GAAI,IAAI,CAACD,UAAS,GAAI,IAAI,CAACH,aAAa,CAACoB,MAAM,CAACC,MAAM,CAACC,IAAI,CAAC,IAAI,CAACtB,aAAa,CAACoB,MAAM,CAAC,CAAC,CAAC,CAAC;MAEzG,CAAC,CAAC;MACFX,KAAK,CACD,+EACJ,EACCC,IAAI,CAAEC,GAAG,IAAIA,GAAG,CAACC,IAAI,CAAC,CAAC,EACvBF,IAAI,CAAEX,IAAI,IAAG;QAEd,IAAI,CAACO,WAAU,GAAI,CAACP,IAAI,CAAC;QACzBwB,OAAO,CAACC,GAAG,CAAC,IAAI,CAAClB,WAAW,CAAC,CAAC,CAAC;MAC/B,CAAC;MACD,IAAI,CAACD,QAAO,GAAI,IAAG;IACvB;EACJ,CAAC;EACDoB,OAAOA,CAAA,EAAE,CACT;AACJ,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}