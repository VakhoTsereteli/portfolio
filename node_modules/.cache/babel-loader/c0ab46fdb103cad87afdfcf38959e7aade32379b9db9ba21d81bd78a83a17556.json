{"ast":null,"code":"import currencies from \"./currencies.json\";\nimport \"./CarencyCalculator.css\";\nimport MainComponent from \"../../components/Main/Main.vue\";\nexport default {\n  name: \"CurrencyCalculator\",\n  components: {\n    MainComponent\n  },\n  data() {\n    return {\n      fromAmount: null,\n      toAmount: null,\n      countryDatas: null,\n      fromSelected: null,\n      showFromMenu: false,\n      toSelected: null,\n      showToMenu: false,\n      currencies: currencies,\n      USD_\n    };\n  },\n  methods: {\n    currencyChange() {\n      fetch(`https://v6.exchangerate-api.com/v6/4f97de937a2f5ca4c59a5c1c/latest/${this.fromSelected && this.fromSelected.currencies[0].code}`).then(res => res.json()).then(data => {\n        this.toAmount = data.conversion_rates[this.toSelected.currencies[0].code];\n        this.toAmount = this.fromAmount * this.toAmount;\n      });\n    },\n    SelectFrom(currency) {\n      this.fromSelected = currency;\n      this.showFromMenu = false;\n    },\n    SelectTo(currency) {\n      this.toSelected = currency;\n      this.showToMenu = false;\n    }\n  },\n  mounted() {\n    this.GetCountryData();\n  },\n  computed: {}\n};","map":{"version":3,"names":["currencies","MainComponent","name","components","data","fromAmount","toAmount","countryDatas","fromSelected","showFromMenu","toSelected","showToMenu","USD_","methods","currencyChange","fetch","code","then","res","json","conversion_rates","SelectFrom","currency","SelectTo","mounted","GetCountryData","computed"],"sources":["/home/vakho_tsereteli/portfolio/src/Projects/CurrencyCalculator/CarencyCalculator.vue"],"sourcesContent":["<template>\n    <MainComponent/>\n    <br>\n    <div class=\"main\">\n        <h1 class=\"title\">Currency Calculator</h1>\n        <div class=\"currencies\">\n            \n        </div>\n        <div class=\"calculator\">\n            <div>\n                <div>\n                    <button class=\"choose\" @click=\"showFromMenu = !showFromMenu\">\n                        <h5 v-if=\"fromSelected\">{{ fromSelected && fromSelected.code }}</h5>\n                        <i class=\"bi bi-caret-up-fill arrow\" v-if=\"showFromMenu\"></i>\n                        <i class=\"bi bi-caret-down-fill arrow\" v-else></i>\n                    </button>\n                    <br>\n                    <div v-show=\"showFromMenu\">\n                        <li v-for=\"currency in currencies\" :key=\"currency\" @click=\"SelectFrom(currency)\">\n                            <h2 style=\"margin-left: 30px;\">{{ currency.flag }}</h2>\n                            <h2 style=\"position: absolute; right: 0;\">\n                                {{ currency.code }}\n                            </h2>\n                        </li>\n                    </div>\n                    <br>\n                    <label>\n                        <h4>Amount From</h4>\n                    </label>\n                    <input type=\"text\" v-model=\"fromAmount\"/>\n                </div>\n                <div>\n                    <button class=\"choose\" @click=\"showToMenu = !showToMenu\">\n                        <img :src=\"toSelected && toSelected.flags.png\" v-if=\"toSelected\" />\n                        {{ toSelected && toSelected.currencies[0].code }}\n                        <i class=\"bi bi-caret-up-fill arrow\" v-if=\"showToMenu\"></i>\n                        <i class=\"bi bi-caret-down-fill arrow\" v-else></i>\n                    </button>\n                    <br>\n                    <div v-show=\"showToMenu\">\n                        <li v-for=\"currency in currencies\" :key=\"currency\" @click=\"SelectTo(currency)\">\n                            <h2 style=\"margin-left: 30px;\">{{ currency.flag }}</h2>\n                            <h2 style=\"position: absolute; right: 0;\">\n                                {{ currency.code }}\n                            </h2>\n                        </li>\n                    </div>\n                    <br>\n                    <label>\n                        <h4>Amount From</h4>\n                    </label>\n                    <input type=\"text\" v-model=\"toAmount\"/>\n                </div>\n            </div>\n            <button @click=\"currencyChange\" id=\"convert\">Convert</button>\n        </div>\n    </div>\n</template>\n\n<script>\n    import currencies from \"./currencies.json\"\n    import \"./CarencyCalculator.css\";\n    import MainComponent from \"../../components/Main/Main.vue\"\n\n    export default{\n        name: \"CurrencyCalculator\",\n        components: {\n            MainComponent\n        },\n        data(){\n            return{\n                fromAmount: null,\n                toAmount: null,\n                countryDatas: null,\n                fromSelected: null,\n                showFromMenu: false,\n                toSelected: null,\n                showToMenu: false,\n                currencies: currencies,\n                USD_\n            };\n        },\n        methods: {\n            currencyChange(){\n                fetch(\n                    `https://v6.exchangerate-api.com/v6/4f97de937a2f5ca4c59a5c1c/latest/${this.fromSelected && this.fromSelected.currencies[0].code}`\n                    )\n                .then((res)=> res.json())\n                .then((data)=> {\n\n                    this.toAmount = data.conversion_rates[this.toSelected.currencies[0].code]\n                    this.toAmount = this.fromAmount * this.toAmount\n                });\n            },\n            SelectFrom(currency){\n                this.fromSelected = currency\n                this.showFromMenu = false\n            },\n            SelectTo(currency){\n                this.toSelected = currency\n                this.showToMenu = false\n            }\n        },\n        mounted(){\n            this.GetCountryData()\n        },\n        computed: {\n        }\n    };\n</script>"],"mappings":"AA4DI,OAAOA,UAAS,MAAO,mBAAkB;AACz