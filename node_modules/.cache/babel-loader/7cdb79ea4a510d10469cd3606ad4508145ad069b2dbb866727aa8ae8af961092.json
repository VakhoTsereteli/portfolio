{"ast":null,"code":"import \"./CarencyCalculator.css\";\nimport MainComponent from \"../../components/Main/Main.vue\";\nexport default {\n  name: \"CurrencyCalculator\",\n  components: {\n    MainComponent\n  },\n  data() {\n    return {\n      currency: {\n        gel: {\n          \"name\": \"GEL\"\n        },\n        usd: {\n          \"name\": \"USD\",\n          \"price\": 20\n        },\n        eur: {\n          \"name\": \"EUR\",\n          \"price\": 10\n        }\n      },\n      currencyDatas: null,\n      fromName: \"USD\",\n      toName: \"GEL\",\n      fromAmount: null,\n      toAmount: null,\n      showData: false\n    };\n  },\n  methods: {\n    currencyData() {\n      fetch(`https://api.fastforex.io/fetch-one?from=${this.fromName}&to=${this.toName}&api_key=67c030db8d-4cf9a781a1-rv2gbf`).then(res => res.json()).then(data => {\n        localStorage.setItem(\"currencyDatas\", JSON.stringify(data));\n        var ParsedCurrencyDatas = JSON.parse(localStorage.getItem(\"currencyDatas\"));\n        this.currencyDatas = ParsedCurrencyDatas;\n        this.showData = true;\n        this.toAmount = this.fromAmount * this.currencyDatas.result[Object.keys(this.currencyDatas.result)[0]];\n      });\n    }\n  },\n  mounted() {}\n};","map":{"version":3,"names":["MainComponent","name","components","data","currency","gel","usd","eur","currencyDatas","fromName","toName","fromAmount","toAmount","showData","methods","currencyData","fetch","then","res","json","localStorage","setItem","JSON","stringify","ParsedCurrencyDatas","parse","getItem","result","Object","keys","mounted"],"sources":["/home/vakho_tsereteli/portfolio/src/Projects/CurrencyCalculator/CarencyCalculator.vue"],"sourcesContent":["<template>\n    <MainComponent/>\n    <br>\n    <div class=\"main\">\n        <h1 class=\"title\">Currency Calculator</h1>\n        <div class=\"calculator\">\n            <br>\n            <div>\n                <li>\n                    <select v-model=\"fromName\">\n            