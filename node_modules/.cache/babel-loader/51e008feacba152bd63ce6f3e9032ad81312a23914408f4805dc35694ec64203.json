{"ast":null,"code":"import \"./CarencyCalculator.css\";\nimport MainComponent from \"../../components/Main/Main.vue\";\nexport default {\n  name: \"CurrencyCalculator\",\n  components: {\n    MainComponent\n  },\n  data() {\n    return {\n      fromAmount: null,\n      toAmount: null,\n      countryDatas: null,\n      fromSelected: null,\n      showFromMenu: false,\n      toSelected: null,\n      showToMenu: false\n    };\n  },\n  methods: {\n    currencyChange() {\n      if (this.fromSelected && this.fromSelected.currencies && this.fromSelected.currencies.length > 0) {\n        const fromCode = this.fromSelected.currencies[0].code;\n        fetch(`https://v6.exchangerate-api.com/v6/4f97de937a2f5ca4c59a5c1c/latest/${fromCode}`).then(res => res.json()).then(data => {\n          if (this.toSelected && this.toSelected.currencies && this.toSelected.currencies.length > 0) {\n            const toCode = this.toSelected.currencies[0].code;\n            this.toAmount = data.conversion_rates[toCode];\n          }\n        });\n      }\n    },\n    GetCountryData() {\n      fetch(\"https://restcountries.com/v2/all\").then(res => res.json()).then(data => {\n        this.countryDatas = data;\n      });\n    },\n    toggleDropdown() {\n      this.isDropdownOpen = !this.isDropdownOpen;\n    },\n    SelectFrom(country) {\n      this.fromSelected = country;\n      this.showFromMenu = false;\n    },\n    SelectTo(country) {\n      this.toSelected = country;\n      this.showToMenu = false;\n    }\n  },\n  mounted() {\n    this.GetCountryData();\n  },\n  computed: {\n    currencyCode() {\n      if (this.countryData && this.countryData.currencies && this.countryData.currencies.length > 0) {\n        return this.countryData.currencies[0].code;\n      }\n      return null;\n    }\n  }\n};","map":{"version":3,"names":["MainComponent","name","components","data","fromAmount","toAmount","countryDatas","fromSelected","showFromMenu","toSelected","showToMenu","methods","currencyChange","currencies","length","fromCode","code","fetch","then","res","json","toCode","conversion_rates","GetCountryData","toggleDropdown","isDropdownOpen","SelectFrom","country","SelectTo","mounted","computed","currencyCode","countryData"],"sources":["/home/vakho_tsereteli/portfolio/src/Projects/CurrencyCalculator/CarencyCalculator.vue"],"sourcesContent":["<template>\n    <MainComponent/>\n    <br>\n    <div class=\"main\">\n        <h1 class=\"title\">Currency Calculator</h1>\n        <div class=\"currencies\">\n            \n        </div>\n        <div class=\"calculator\">\n            <div>\n                <div>\n                    <button class=\"choose\" @click=\"showFromMenu = !showFromMenu\">\n                        <img :src=\"fromSelected && fromSelected.flags.png\" v-if=\"fromSelected\" />\n                        {{ fromSelected && fromSelected.currencies[0].code }}\n                        <i class=\"bi bi-caret-up-fill arrow\" v-if=\"showFromMenu\"></i>\n                        <i class=\"bi bi-caret-down-fill arrow\" v-else></i>\n                    </button>\n                    <br>\n                    <div v-show=\"showFromMenu\">\n                        <li v-for=\"country in countryDatas\" :key=\"country\" @click=\"SelectFrom(country)\">\n                            <img :src=\"country.flags.png\"/>\n                            <h2 v-if=\"country.currencies && country.currencies.length > 0\">\n                                {{country.currencies[0].code }}\n                            </h2>\n                        </li>\n                    </div>\n                    <br>\n                    <label>\n                        <h4>Amount From</h4>\n                    </label>\n                    <input type=\"text\" v-model=\"fromAmount\"/>\n                </div>\n                <div>\n                    <button class=\"choose\" @click=\"showToMenu = !showToMenu\">\n                        <img :src=\"toSelected && toSelected.flags.png\" v-if=\"toSelected\" />\n                        {{ toSelected && toSelected.currencies[0].code }}\n                        <i class=\"bi bi-caret-up-fill arrow\" v-if=\"showToMenu\"></i>\n                        <i class=\"bi bi-caret-down-fill arrow\" v-else></i>\n                    </button>\n                    <br>\n                    <div v-show=\"showToMenu\">\n                        <li v-for=\"country in countryDatas\" :key=\"country\" @click=\"SelectTo(country)\">\n                            <img :src=\"country.flags.png\"/>\n                            <h2 v-if=\"country.currencies && country.currencies.length > 0\">\n                                {{country.currencies[0].code }}\n                            </h2>\n                        </li>\n                    </div>\n                    <br>\n                    <label>\n                        <h4>Amount From</h4>\n                    </label>\n                    <input type=\"text\" v-model=\"toAmount\"/>\n                </div>\n            </div>\n            <button @click=\"currencyChange\" id=\"convert\">Convert</button>\n        </div>\n    </div>\n</template>\n\n<script>\n    import \"./CarencyCalculator.css\";\n    import MainComponent from \"../../components/Main/Main.vue\"\n\n    export default{\n        name: \"CurrencyCalculator\",\n        components: {\n            MainComponent\n        },\n        data(){\n            return{\n                fromAmount: null,\n                toAmount: null,\n                countryDatas: null,\n                fromSelected: null,\n                showFromMenu: false,\n                toSelected: null,\n                showToMenu: false\n            };\n        },\n        methods: {\n            currencyChange() {\n                if (this.fromSelected && this.fromSelected.currencies && this.fromSelected.currencies.length > 0) {\n                    const fromCode = this.fromSelected.currencies[0].code;\n                    fetch(`https://v6.exchangerate-api.com/v6/4f97de937a2f5ca4c59a5c1c/latest/${fromCode}`)\n                    .then((res) => res.json())\n                    .then((data) => {\n                        if (this.toSelected && this.toSelected.currencies && this.toSelected.currencies.length > 0) {\n                        const toCode = this.toSelected.currencies[0].code;\n                        this.toAmount = data.conversion_rates[toCode];\n                        }\n                    });\n                }\n            },\n                        GetCountryData() {\n                fetch(\"https://restcountries.com/v2/all\")\n                    .then((res) => res.json())\n                    .then((data) => {\n                    this.countryDatas = data;\n                    });\n            },\n            toggleDropdown() {\n            this.isDropdownOpen = !this.isDropdownOpen;\n            },\n            SelectFrom(country){\n                this.fromSelected = country\n                this.showFromMenu = false\n            },\n            SelectTo(country){\n                this.toSelected = country\n                this.showToMenu = false\n            }\n        },\n        mounted(){\n            this.GetCountryData()\n        },\n        computed: {\n            currencyCode() {\n            if (this.countryData && this.countryData.currencies && this.countryData.currencies.length > 0) {\n                return this.countryData.currencies[0].code;\n            }\n            return null;\n            }\n        }\n    };\n</script>"],"mappings":"AA6DI,OAAO,yBAAyB;AAChC,OAAOA,aAAY,MAAO,gCAA+B;AAEzD,eAAc;EACVC,IAAI,EAAE,oBAAoB;EAC1BC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAIA,CAAA,EAAE;IACF,OAAM;MACFC,UAAU,EAAE,IAAI;MAChBC,QAAQ,EAAE,IAAI;MACdC,YAAY,EAAE,IAAI;MAClBC,YAAY,EAAE,IAAI;MAClBC,YAAY,EAAE,KAAK;MACnBC,UAAU,EAAE,IAAI;MAChBC,UAAU,EAAE;IAChB,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACLC,cAAcA,CAAA,EAAG;MACb,IAAI,IAAI,CAACL,YAAW,IAAK,IAAI,CAACA,YAAY,CAACM,UAAS,IAAK,IAAI,CAACN,YAAY,CAACM,UAAU,CAACC