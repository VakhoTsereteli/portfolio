{"ast":null,"code":"import currencies from \"./currencies.json\";\nimport \"./CarencyCalculator.css\";\nimport MainComponent from \"../../components/Main/Main.vue\";\nconst test = 10 * 5;\nexport default {\n  name: \"CurrencyCalculator\",\n  components: {\n    MainComponent\n  },\n  data() {\n    return {\n      fromAmount: null,\n      toAmount: null,\n      countryDatas: null,\n      fromSelected: null,\n      showFromMenu: true,\n      toSelected: null,\n      showToMenu: false,\n      currencies: currencies,\n      test2: test,\n      USD_GBP_EUR: {\n        \"EUR\": {\n          \"cost\": 2.7,\n          \"code\": \"EUR\",\n          \"flag\": \"ðŸ‡ªðŸ‡º\"\n        },\n        \"USD\": {\n          \"cost\": 2.5,\n          \"code\": \"USD\",\n          \"flag\": \"ðŸ‡ºðŸ‡¸\"\n        },\n        \"GBP\": {\n          \"cost\": 3.2,\n          \"code\": \"GBP\",\n          \"flag\": \"ðŸ‡¬ðŸ‡§\"\n        }\n      }\n    };\n  },\n  methods: {\n    currencyChange() {\n      fetch(`https://v6.exchangerate-api.com/v6/4f97de937a2f5ca4c59a5c1c/latest/${this.fromSelected && this.fromSelected.currencies[0].code}`).then(res => res.json()).then(data => {\n        this.toAmount = data.conversion_rates[this.toSelected.currencies[0].code];\n        this.toAmount = this.fromAmount * this.toAmount;\n      });\n    },\n    SelectFrom(currency) {\n      this.fromSelected = currency;\n      this.showFromMenu = false;\n    },\n    SelectTo(currency) {\n      this.toSelected = currency;\n      this.showToMenu = false;\n    }\n  },\n  mounted() {\n    this.GetCountryData();\n  },\n  computed: {}\n};","map":{"version":3,"names":["currencies","MainComponent","test","name","components","data","fromAmount","toAmount","countryDatas","fromSelected","showFromMenu","toSelected","showToMenu","test2","USD_GBP_EUR","methods","currencyChange","fetch","code","then","res","json","conversion_rates","SelectFrom","currency","SelectTo","mounted","GetCountryData","computed"],"sources":["/home/vakho_tsereteli/portfolio/src/Projects/CurrencyCalculator/CarencyCalculator.vue"],"sourcesContent":["<template>\n    <MainComponent/>\n    <br>\n    <div class=\"main\">\n        <h1 class=\"title\">Currency Calculator</h1>\n        <div class=\"currencies\">\n            <div v-for=\"currency in USD_GBP_EUR\" :key=\"currency\">\n                <h1>{{ currency.flag+\" \"+currency.code }}</h1>\n                <h1 style=\"margin-top: 10px;\">{{ currency.cost }}</h1>\n            </div>\n        </div>\n        <div class=\"calculator\">\n            <div>\n                <div>\n                    <button class=\"choose\" @v>\n                        <h5 v-if=\"fromSelected\">{{ fromSelected && fromSelected.code }}</h5>\n                        <i class=\"bi bi-caret-up-fill arrow\" v-if=\"showFromMenu\"></i>\n                        <i class=\"bi bi-caret-down-fill arrow\" v-else></i>\n                    </button>\n                    <br>\n                    <div v-show=\"showFromMenu\">\n                        <li v-for=\"currency in currencies\" :key=\"currency\" @click=\"SelectFrom(currency)\">\n                            <h2 style=\"margin-left: 30px;\">{{ currency.flag }}</h2>\n                            <h2 style=\"position: absolute; right: 0;\">\n                                {{ currency.code }}\n                            </h2>\n                        </li>\n                    </div>\n                    <br>\n                    <label>\n                        <h4>From</h4>\n                    </label>\n                    <input type=\"text\" v-model=\"fromAmount\"/>\n                </div>\n                <div>\n                    <button class=\"choose\" @click=\"showToMenu = !showToMenu\">\n                        <img :src=\"toSelected && toSelected.flags.png\" v-if=\"toSelected\" />\n                        {{ toSelected && toSelected.currencies[0].code }}\n                        <i class=\"bi bi-caret-up-fill arrow\" v-if=\"showToMenu\"></i>\n                        <i class=\"bi bi-caret-down-fill arrow\" v-else></i>\n                    </button>\n                    <br>\n                    <div v-show=\"showToMenu\">\n                        <li v-for=\"currency in currencies\" :key=\"currency\" @click=\"SelectTo(currency)\">\n                            <h2 style=\"margin-left: 30px;\">{{ currency.flag }}</h2>\n                            <h2 style=\"position: absolute; right: 0;\">\n                                {{ currency.code }}\n                            </h2>\n                        </li>\n                    </div>\n                    <br>\n                    <label>\n                        <h4>To</h4>\n                    </label>\n                    <input type=\"text\" v-model=\"toAmount\"/>\n                </div>\n            </div>\n            <button @click=\"currencyChange\" id=\"convert\">Convert</button>\n        </div>\n        <h1 >{{ Object.keys(currencies).length }}</h1>\n    </div>\n</template>\n\n<script>\n    import currencies from \"./currencies.json\";\n    import \"./CarencyCalculator.css\";\n    import MainComponent from \"../../components/Main/Main.vue\";\n\n    const test = 10 * 5\n\n    export default{\n        name: \"CurrencyCalculator\",\n        components: {\n            MainComponent\n        },\n        data(){\n            return{\n                fromAmount: null,\n                toAmount: null,\n                countryDatas: null,\n                fromSelected: null,\n                showFromMenu: true,\n                toSelected: null,\n                showToMenu: false,\n                currencies: currencies,\n                test2: test,\n                USD_GBP_EUR:{\n                    \"EUR\": {\"cost\": 2.7, \"code\": \"EUR\", \"flag\": \"ðŸ‡ªðŸ‡º\"},\n                    \"USD\": {\"cost\": 2.5, \"code\": \"USD\", \"flag\": \"ðŸ‡ºðŸ‡¸\"},\n                    \"GBP\": {\"cost\": 3.2, \"code\": \"GBP\", \"flag\": \"ðŸ‡¬ðŸ‡§\"}\n                }\n            };\n        },\n        methods: {\n            currencyChange(){\n                fetch(\n                    `https://v6.exchangerate-api.com/v6/4f97de937a2f5ca4c59a5c1c/latest/${this.fromSelected && this.fromSelected.currencies[0].code}`\n                    )\n                .then((res)=> res.json())\n                .then((data)=> {\n\n                    this.toAmount = data.conversion_rates[this.toSelected.currencies[0].code]\n                    this.toAmount = this.fromAmount * this.toAmount\n                });\n            },\n            SelectFrom(currency){\n                this.fromSelected = currency\n                this.showFromMenu = false\n            },\n            SelectTo(currency){\n                this.toSelected = currency\n                this.showToMenu = false\n            }\n        },\n        mounted(){\n            this.GetCountryData()\n        },\n        computed: {\n        }\n    };\n</script>"],"mappings":"AAgEI,OAAOA,UAAS,MAAO,mBAAmB;AAC1C,OAAO,yBAAyB;AAChC,OAAOC,aAAY,MAAO,gCAAgC;AAE1D,MAAMC,IAAG,GAAI,EAAC,GAAI;AAElB,eAAc;EACVC,IAAI,EAAE,oBAAoB;EAC1BC,UAAU,EAAE;IACRH;EACJ,CAAC;EACDI,IAAIA,CAAA,EAAE;IACF,OAAM;MACFC,UAAU,EAAE,IAAI;MAChBC,QAAQ,EAAE,IAAI;MACdC,YAAY,EAAE,IAAI;MAClBC,YAAY,EAAE,IAAI;MAClBC,YAAY,EAAE,IAAI;MAClBC,UAAU,EAAE,IAAI;MAChBC,UAAU,EAAE,KAAK;MACjBZ,UAAU,EAAEA,UAAU;MACtBa,KAAK,EAAEX,IAAI;MACXY,WAAW,EAAC;QACR,KAAK,EAAE;UAAC,MAAM,EAAE,GAAG;UAAE,MAAM,EAAE,KAAK;UAAE,MAAM,EAAE;QAAM,CAAC;QACnD,KAAK,EAAE;UAAC,MAAM,EAAE,GAAG;UAAE,MAAM,EAAE,KAAK;UAAE,MAAM,EAAE;QAAM,CAAC;QACnD,KAAK,EAAE;UAAC,MAAM,EAAE,GAAG;UAAE,MAAM,EAAE,KAAK;UAAE,MAAM,EAAE;QAAM;MACtD;IACJ,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACLC,cAAcA,CAAA,EAAE;MACZC,KAAK,CACA,sEAAqE,IAAI,CAACR,YAAW,IAAK,IAAI,CAACA,YAAY,CAACT,UAAU,CAAC,CAAC,CAAC,CAACkB,IAAK,EAChI,EACHC,IAAI,CAAEC,GAAG,IAAIA,GAAG,CAACC,IAAI,CAAC,CAAC,EACvBF,IAAI,CAAEd,IAAI,IAAI;QAEX,IAAI,CAACE,QAAO,GAAIF,IAAI,CAACiB,gBAAgB,CAAC,IAAI,CAACX,UAAU,CAACX,UAAU,CAAC,CAAC,CAAC,CAACkB,IAA