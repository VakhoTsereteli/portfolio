{"ast":null,"code":"import \"./CarencyCalculator.css\";\nimport MainComponent from \"../../components/Main/Main.vue\";\nexport default {\n  name: \"CurrencyCalculator\",\n  components: {\n    MainComponent\n  },\n  data() {\n    return {\n      currency: {\n        gel: {\n          \"name\": \"GEL\"\n        },\n        usd: {\n          \"name\": \"USD\",\n          \"price\": 20\n        },\n        eur: {\n          \"name\": \"EUR\",\n          \"price\": 10\n        }\n      },\n      currencyDatas: \"\",\n      fromName: null,\n      toName: null,\n      fromAmount: null,\n      toAmount: null,\n      showData: false\n    };\n  },\n  methods: {\n    currencyData() {\n      fetch(`https://api.fastforex.io/fetch-one?from=${this.fromName}&to=${this.toName}&api_key=67c030db8d-4cf9a781a1-rv2gbf`).then(res => res.json()).then(data => {\n        localStorage.setItem(\"currencyData\", JSON.stringify(data));\n        var ParsedCurrencyData = JSON.parse(localStorage.getItem(\"currencyDatas\"));\n        this.currencyDatas = ParsedCurrencyData;\n        if (currencyData && currencyData.result) {\n          var resultKeys = Object.keys(currencyData.result);\n          if (resultKeys.length > 0) {\n            var index = 0; // Change this to the desired index\n            var key = resultKeys[index];\n            var value = currencyData.result[key];\n            console.log(value);\n          }\n        }\n        this.showData = true;\n      });\n    }\n  },\n  mounted() {}\n};","map":{"version":3,"names":["MainComponent","name","components","data","currency","gel","usd","eur","currencyDatas","fromName","toName","fromAmount","toAmount","showData","methods","currencyData","fetch","then","res","json","localStorage","setItem","JSON","stringify","ParsedCurrencyData","parse","getItem","result","resultKeys","Object","keys","length","index","key","value","console","log","mounted"],"sources":["/home/vakho_tsereteli/portfolio/src/Projects/CurrencyCalculator/CarencyCalculator.vue"],"sourcesContent":["<template>\n    <MainComponent/>\n    <br>\n    <div class=\"main\">\n        <h1 class=\"title\">Currency Calculator</h1>\n        <div class=\"calculator\">\n            <br>\n            <div>\n                <li>\n                    <select v-model=\"fromName\">\n                        <option v-for=\"(currency, index) in currency\" :key=\"index\">{{ currency.name }}</option>\n                    </select>\n                    <br>\n                    <input type=\"text\"/>\n                </li>\n                <li>\n                    <select v-model=\"toName\">\n                        <option v-for=\"(currency, index) in currency\" :key=\"index\">{{ currency.name }}</option>\n                    </select>\n                    <br>\n                    <input type=\"text\"/>\n                </li>\n            </div>\n            <button @click=\"currencyData\" id=\"convert\">Convert</button>\n        </div>\n        <h1 v-if=\"showData\">{{ currencyDatas }}</h1>\n    </div>\n</template>\n\n<script>\n    import \"./CarencyCalculator.css\";\n    import MainComponent from \"../../components/Main/Main.vue\"\n\n    export default{\n        name: \"CurrencyCalculator\",\n        components: {\n            MainComponent\n        },\n        data(){\n            return{\n                currency:{\n                    gel:{\"name\": \"GEL\", },\n                    usd:{\"name\": \"USD\", \"price\": 20},\n                    eur:{\"name\": \"EUR\", \"price\": 10}\n                },\n                currencyDatas: \"\",\n                fromName: null,\n                toName: null,\n                fromAmount: null,\n                toAmount: null,\n                showData: false\n            };\n        },\n        methods: {\n            currencyData(){\n                fetch(\n                    `https://api.fastforex.io/fetch-one?from=${this.fromName}&to=${this.toName}&api_key=67c030db8d-4cf9a781a1-rv2gbf`\n                    )\n                .then((res)=> res.json())\n                .then((data)=> {\n                    localStorage.setItem(\"currencyData\", JSON.stringify(data));\n                    var ParsedCurrencyData = JSON.parse(localStorage.getItem(\"currencyDatas\"));\n\n                    this.currencyDatas = ParsedCurrencyData;\n                    if (currencyData && currencyData.result) {\n                var resultKeys = Object.keys(currencyData.result);\n                if (resultKeys.length > 0) {\n                    var index = 0; // Change this to the desired index\n                    var key = resultKeys[index];\n                    var value = currencyData.result[key];\n                    console.log(value);\n                }\n                }\n                    this.showData = true\n                });\n            },\n        },\n        mounted(){\n        }\n    };\n</script>"],"mappings":"AA8BI,OAAO,yBAAyB;AAChC,OAAOA,aAAY,MAAO,gCAA+B;AAEzD,eAAc;EACVC,IAAI,EAAE,oBAAoB;EAC1BC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAIA,CAAA,EAAE;IACF,OAAM;MACFC,QAAQ,EAAC;QACLC,GAAG,EAAC;UAAC,MAAM,EAAE;QAAO,CAAC;QACrBC,GAAG,EAAC;UAAC,MAAM,EAAE,KAAK;UAAE,OAAO,EAAE;QAAE,CAAC;QAChCC,GAAG,EAAC;UAAC,MAAM,EAAE,KAAK;UAAE,OAAO,EAAE;QAAE;MACnC,CAAC;MACDC,aAAa,EAAE,EAAE;MACjBC,QAAQ,EAAE,IAAI;MACdC,MAAM,EAAE,IAAI;MACZC,UAAU,EAAE,IAAI;MAChBC,QAAQ,EAAE,IAAI;MACdC,QAAQ,EAAE;IACd,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACLC,YAAYA,CAAA,EAAE;MACVC,KAAK,CACA,2CAA0C,IAAI,CAACP,QAAS,OAAM,IAAI,CAACC,MAAO,uCAC3E,EACHO,IAAI,CAAEC,GAAG,IAAIA,GAAG,CAACC,IAAI,CAAC,CAAC,EACvBF,IAAI,CAAEd,IAAI,IAAI;QACXiB,YAAY,CAACC,OAAO,CAAC,cAAc,EAAEC,IAAI,CAACC,SAAS,CAACpB,IAAI,CAAC,CAAC;QAC1D,IAAIqB,kBAAiB,GAAIF,IAAI,CAACG,KAAK,CAACL,YAAY,CAACM,OAAO,CAAC,eAAe,CAAC,CAAC;QAE1E,IAAI,CAAClB,aAAY,GAAIgB,kBAAkB;QACvC,IAAIT,YAAW,IAAKA,YAAY,CAACY,MAAM,EAAE;UAC7C,IAAIC,UAAS,GAAIC,MAAM,CAACC,IAAI,CAACf,YAAY,CAACY,MAAM,CAAC;UACjD,IAAIC,UAAU,CAACG,MAAK,GAAI,CAAC,EAAE;YACvB,IAAIC,KAAI,GAAI,CAAC,EAAE;YACf,IAAIC,GAAE,GAAIL,UAAU,CAACI,KAAK,C